{
  "manifest_version": 2,
  "name": "To DeepL",
  "version": "0.4",
  "description": "Translate selected text in new tab with DeepL",
  "homepage_url": "https://github.com/rewkha/chrome-to-deepl",
  "icons": {
    "48": "icons/icon.svg"
  },
  "permissions": ["activeTab", "contextMenus", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "browser_style": true,
    "page": "options/options.html",
    "open_in_tab": false
  }
}
